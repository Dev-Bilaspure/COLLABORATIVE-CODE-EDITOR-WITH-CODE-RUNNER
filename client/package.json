{
  "name": "cpd-navigator-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "yarn build && firebase deploy --only hosting",
    "tsc": "tsc",
    "lint": "eslint ./src --ext .tsx --ext .ts --ext .jsx --ext .js",
    "lint:fix": "eslint ./src --ext .tsx --ext .ts --ext .jsx --ext .js --fix",
    "test": "yarn lint && yarn tsc",
    "test:emu": "firebase emulators:start --project=cpd-navigator",
    "start:dev": "concurrently \"yarn test:emu\" \"yarn dev\"",
    "start:staging": "concurrently \"yarn test:emu\" \"yarn run:staging\"",
    "playwright:run": "cd playwright && npx playwright test",
    "playwright:ci": "start-server-and-test start:dev http://localhost:3000 playwright:run",
    "dev:staging": "vite --mode staging --open",
    "dev:production": "vite --mode production --open",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@monaco-editor/react": "^4.5.1",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "axios": "^1.3.4",
    "buffer": "^6.0.3",
    "concurrently": "^7.6.0",
    "firebase": "^9.17.0",
    "immer": "^9.0.19",
    "js-cookie": "^3.0.5",
    "js-file-download": "^0.4.12",
    "lodash.debounce": "^4.0.8",
    "mongoose": "^7.4.0",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.43.0",
    "react-router-dom": "^6.8.0",
    "socket.io-client": "^4.7.1",
    "start-server-and-test": "^2.0.0",
    "tailwind-merge": "^1.12.0",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-eslint": "^1.8.1"
  },
  "packageManager": "yarn@1.22.19"
}
